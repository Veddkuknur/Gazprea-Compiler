procedure main() returns integer {

    // casting to integer
    as<integer>(1.2) -> std_output;
    as<integer>(3.14129387) -> std_output;
    as<integer>('a') -> std_output;
    as<integer>('A') -> std_output;
    as<integer>(true) -> std_output;
    as<integer>(false) -> std_output;
    as<integer>(123) -> std_output;

    '\n' -> std_output;

    // casting to real
    as<real>(12) -> std_output;
    as<real>(0) -> std_output;
    as<real>(1.2) -> std_output;
    as<real>('a') -> std_output;
    as<real>(true) -> std_output;
    as<real>(false) -> std_output;

    '\n' -> std_output;

    // casting to boolean
    as<boolean>(12) -> std_output;
    as<boolean>(0) -> std_output;
    as<boolean>('a') -> std_output;
    as<boolean>(true) -> std_output;
    as<boolean>(false) -> std_output;

    '\n' -> std_output;

    // casting to char
    as<character>(12) -> std_output;
    as<character>(0) -> std_output;
    as<character>('a') -> std_output;
    as<character>(true) -> std_output;
    as<character>(false) -> std_output;

    '\n' -> std_output;

    return 0;
}

